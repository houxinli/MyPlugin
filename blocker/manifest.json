{
  "name": "blocker",
  "manifest_version": 2,
  "version": "0.1.0",
  "description": "Block CSDN sideblock",
  "browser_action": {
    "default_icon":{
      "16": "icon/circle-thin-16.png",
      "32": "icon/circle-thin-32.png",
      "128": "icon/circle-thin-128.png"
    },
    "default_title": "blocker by houxinli"
  },
  "content_scripts": [
    {
      "matches": ["https://blog.csdn.net/*"],
      "js": [
        "js/jquery.js",
        "js/blocker.js"
      ],
      "css": [
        "css/blocker.css"
      ]
    }
  ]
}
